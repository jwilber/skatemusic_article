.waffle_col{
    float: left;
    width: 24%;
    height: 50%;
    /*border: 1px solid blue;*/
    overflow: hidden;
    margin: 20px 5px;
}

.waffle_row{
    width: 100%;
    height: 50%;
    /*margin: 50px 10px;*/
    /*border: 5px solid red;*/
}

.waffle_title {
    font-size: 20px;
    position: relative;
    left: 45%;
    -webkit-transform: translateX(-45%);
    -ms-transform: translateX(-45%);
    transform: translateX(-45%);
    text-align: left;
    font-family: Gill Sans;
}

.waffle_main_title {
    font-family: Gill Sans;
    font-size: 50px;
}

.legend {
padding-bottom: 7px;
}

.legend_item {
display: inline-block;
padding-right: 10px;
margin-bottom: 2px;
}

.legend_item_icon, .legend_item_text {
display: inline-block;
vertical-align: middle;
font-size: 25px;
}

.legend_item_icon {
margin-right: 15px;
height: 10px;
width: 10px;
}

.waffle_list {
    margin-left: 40px;
    list-style-type: circle;
}
